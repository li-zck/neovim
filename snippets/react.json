{
  "Arrow function": {
    "prefix": "afn",
    "body": [
      "const ${1:name} = ($2)${3|, : void, |} => {",
      "  $4",
      "}"
    ],
    "description": "Arrow function snippet"
  },
  "Try catch block": {
    "prefix": "tc",
    "body": [
      "try {",
      "  $1",
      "} catch (${2:error}) {",
      "  $3",
      "}"
    ]
  }
}
